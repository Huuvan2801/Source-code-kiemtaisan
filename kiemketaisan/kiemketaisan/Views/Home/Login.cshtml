@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Quản lý nhân sự</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/assets/css/dialog.css" rel="stylesheet" />
    <link href="~/assets/css/messagebox.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <title>Quản lý nhân sự</title>
    <link rel="shortcut icon" href="~/assets/images/icon-10.png" />
    <link href="~/assets/css/util.css" rel="stylesheet" />
    <link href="~/assets/css/main.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/fonts/iconic/css/material-design-iconic-font.min.css">
</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('../../assets/images/bg-01.jpg');">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                @using (Ajax.BeginForm("Login", "Home", null,
     new AjaxOptions
     {
         HttpMethod = "Post",
         OnSuccess = "login",
         UpdateTargetId = "response",
         InsertionMode = InsertionMode.Replace,
     },
     new { enctype = "multipart/form-data" }
     ))
                {
                    <span class="login100-form-title p-b-49">
                        Đăng nhập hệ thống
                    </span>

                    <div class="wrap-input100 validate-input m-b-23" data-validate="Username is reauired">
                        <span class="label-input100">Tên đăng nhập</span>
                        <input class="input100" type="text" name="TenDangNhap" placeholder="Tên đăng nhập của bạn">
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <span class="label-input100">Mật khẩu</span>
                        <input class="input100" type="password" name="MatKhau" placeholder="Mật khẩu của bạn" />
                        <span class="focus-input100" data-symbol="&#xf190;"></span>
                    </div>

                    <div class="container-login100-form-btn" style="margin-top:20px">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button class="login100-form-btn" type="submit">
                                Đăng nhập
                            </button>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>

    <div class="dialog" id="responsive">
    </div>
    <div class="message-model">
        <div class="message-content">
        </div>
        <div class="message-box">
            <div class="message-header">
                Thông báo
            </div>
            <div class="message-main">
                <p id="notify"></p>
            </div>
        </div>
    </div>
    <div class="message-delete">
        <div class="message-content">
        </div>
        <div class="message-box">
            <div class="message-header">
                Thông báo
            </div>
            <div class="message-main">
                <p id="xoa">Bạn có chắc chắc muốn xóa ?</p>
            </div>
            <div class="message-footer">
                <button class="btn btn-success huy">Hủy</button>
                <button class="btn btn-primary xoa" style="margin-left:10px">Xóa</button>
            </div>
        </div>
    </div>
    <!-- container-scroller -->
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <!-- plugins:js -->
    <script src="~/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/assets/js/jquery.cookie.js" type="text/javascript"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/assets/js/off-canvas.js"></script>
    <script src="~/assets/js/hoverable-collapse.js"></script>
    @*<script src="~/assets/js/misc.js"></script>*@
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="~/assets/js/common.js"></script>

    <script src="~/assets/asset/vendor/libs/jquery/jquery.js"></script>
    <script src="~/assets/asset/vendor/libs/popper/popper.js"></script>
    <script src="~/assets/asset/vendor/js/bootstrap.js"></script>
    <script src="~/assets/asset/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/assets/asset/vendor/js/menu.js"></script>
    <script src="~/assets/asset/vendor/libs/apex-charts/apexcharts.js"></script>
    <script src="~/assets/asset/js/main.js"></script>
    <script src="~/assets/asset/js/dashboards-analytics.js"></script>
    <script src="~/assets/asset/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/assets/asset/js/ckeditor/ckeditor.js"></script>
    <script src="~/assets/asset/js/ckfinder/ckfinder.js"></script>
    <script src="~/assets/asset/js/ckfinder/config.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.1.0-rc.0/js/select2.min.js" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

</body>
</html>